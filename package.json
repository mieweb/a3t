{
  "name": "a3t",
  "version": "1.0.0",
  "description": "Universal, context-aware asset loader for JavaScript and TypeScript applications with hierarchical lookups, caching, and context sensitivity",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "test": "node --test tests/*.test.js",
    "test:coverage": "node --test --experimental-test-coverage tests/*.test.js",
    "test:mongodb": "node --test tests/*.test.js",
    "test:deno": "deno test --allow-read test-deno.ts",
    "lint": "echo 'Linting would go here - consider adding eslint'",
    "build": "echo 'No build step required for this package'",
    "example": "node example.js",
    "example:git": "node example-git.js",
    "example:deno": "deno run --allow-read --allow-net example-deno.ts"
  },
  "keywords": [
    "asset-loader",
    "configuration",
    "i18n",
    "templates",
    "caching",
    "overrides",
    "universal-loader",
    "git-integration",
    "secret-store",
    "distributed-assets"
  ],
  "author": "Medical Informatics Engineering, Inc.",
  "license": "MPL-2.0",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "mongodb": "^6.17.0",
    "pino": "^9.4.0",
    "simple-git": "^3.28.0"
  },
  "peerDependenciesMeta": {
    "mongodb": {
      "optional": true
    },
    "pino": {
      "optional": true
    }
  }
}
